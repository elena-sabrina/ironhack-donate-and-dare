{(dare.status === "dare-sent" && (
  <>
    <h1>Donor: You have dared {dare.dared.name} </h1>
    <p>Status: {dare.status}</p>
  </>
)) ||
  (dare.status === "video-uploaded" && (
    <>
      <h1>Donor: {dare.dared.name} has uploaded the dare</h1>
      <p>Status: {dare.status}</p>
    </>
  )) ||
  (dare.status === "confirmed" && (
    <>
      <h1>Donor: You have confirmed {dare.dared.name}'s dare</h1>
      <p>Status: {dare.status}</p>
    </>
  )) || <p>Error no dare found</p>}